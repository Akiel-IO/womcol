                                <script>
                                    let cajasPospagoCat = [...document.querySelectorAll('.cajaProducto')];
                                    cajasPospagoCat.forEach(e => {
                                        e.addEventListener('click', clickPospago)
                                    });
                                    function clickPospago(productObj) {

                                        window.dataLayer = window.dataLayer || [];
                                        window.dataLayer.push({
                                            'event': 'productClick',
                                            'ecommerce': {
                                                'click': {
                                                    'actionField': {
                                                        'list': 'Pospago'
                                                    },
                                                    'products': [{
                                                        'name': this.querySelector('.titulo').textContent + '-categoria',
                                                        'id': this.id + '-categoria',
                                                        'price': this.querySelector('.precio').textContent,
                                                        'brand': 'WOM',
                                                        'category': 'Pospago',
                                                        'variant': this.querySelector('.titulo span').textContent,
                                                        'list': 'Pospago',
                                                        'position': '',
                                                    }]
                                                }
                                            },
                                            'eventCallback': function () {
                                                document.location = productObj.url;
                                                window.location.href = "https://www.wom.co/planes-pospago/te-llamamos";
                                            }
                                        });
                                    }

                                </script>
                            </div>
