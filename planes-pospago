                                    defer=""></script>
                                <script>
                                    require(['jquery'], function ($) {
                                        $("#womFormSilde").submit(function (e) {
                                            var captchResponse = $('#g-recaptcha-response-1').val();
                                            if (captchResponse.length == '0') {
                                                //alert("Se requiere validación de captcha.");
                                                $(".validationMessage").html('');
                                                $('.captcha').after(
                                                    "<span class='validationMessage' style='color:red;'> Este campo es requerido.</span>"
                                                );
                                                e.preventDefault();
                                                return false;
                                            } else {
                                                //alert("Gracias");
                                                $(".validationMessage").html('');
                                                return true;
                                            }
                                        });

                                    });
                                </script>
                                <script>
                                    requirejs([
                                        'jquery', 'jquery/validate',
                                    ], function ($) {
                                        $("#check").click(function () {
                                            check = $('#check').val();
                                            if (check == 'false') {
                                                $('#check').val('true');
                                            } else {
                                                $('#check').val('false');
                                            }
                                        });

                                        $('#womFormSilde').validate({
                                            rules: {
                                                nombres: {
                                                    required: true,
                                                    number: false,
                                                    lettersonly: true
                                                },

                                                correoE: {
                                                    required: true,
                                                    betterEmail: true,
                                                },
                                                phone: {
                                                    required: true,
                                                    number: true,
                                                    maxlength: 10,
                                                    minlength: 10,
                                                    phoneStarting3: true
                                                },
                                                actualmenteTengo: {
                                                    valueNotEquals: "default"
                                                },
                                                operadorActual: {
                                                    valueNotEquals: "default"
                                                },
                                                promo: {
                                                    valueNotEquals: "default"
                                                },
                                                Acepta: {
                                                    required: true
                                                },
                                            },
                                            messages: {
                                                nombres: {
                                                    required: "Este campo es requerido.",
                                                    number: "Esta permitido solo letras.",
                                                    lettersonly: "Solo letras SIN espacios al final.",
                                                },
                                                correoE: {
                                                    required: "Este campo es requerido.",
                                                    betterEmail: "Ingresa un correo valido."
                                                },
                                                phone: {
                                                    phoneStarting3: "El número debe comenzar con 3",
                                                    required: "Este campo es requerido.",
                                                    maxlength: "Máximo 10 números",
                                                    minlength: "Mínimo 10 números",
                                                },
                                                actualmenteTengo: "Selecciona tu plan actual",
                                                operadorActual: "Selecciona tu operador",
                                                promo: "Elige un tipo de promoción",
                                                check: {
                                                    required: "Este campo es requerido.",
                                                },

                                            },

                                            submitHandler: function (form) {
                                                const urlactual = window.location.href;
                                                var captchResponse = $('.gecap-res #g-recaptcha-response-1').val();
                                                if (captchResponse.length == '0') {
                                                    //alert("Se requiere validación de captcha.");
                                                    $(".validationMessage").html('');
                                                    $('.captcha').after("<span class='validationMessage' style='color:red;'> Este campo es requerido.</span>");
                                                    //e.preventDefault();
                                                    return false;
                                                } else {
                                                    //alert("Gracias");
                                                    $(".validationMessage").html('');
                                                    //return true;
                                                }

                                                const data = {
                                                    serviceToken: '8d0fce502850205b214e026dfd011cdf',
                                                    serviceAction: 'c2c',
                                                    contactData: {
                                                        firstname: $('#nombres').val(),
                                                        email: $('#correoE').val(),
                                                        phone: $('#phone').val(),
                                                        OperadorActual: $('#operadorActual1').val(),
                                                        estoyInteresadoEn: $('#promo1').val(),
                                                        actualmenteTengo: $('#actualmenteTengo1').val(),
                                                        AceptaTerminos: $('#check').val(),
                                                        formName: 'Modal_Lead_Ecommerce_Mag',
                                                        formUrl: urlactual
                                                    }
                                                }
                                                // TAG MANAGER DATA 
                                                dataLayer.push({
                                                    'event': 'btn-enviar-c2c-slideLeads-mag',
                                                });
                                                // RESPUESTA POSITIVA
                                                let respuesta = document.querySelector('.womL-form');
                                                let okRespuesta = `<div id="OkModalLeadsHome" class="content">
                    <img src="https://app.wom.co/campaign/images/ecommerce/imgs/generales/ok-response.png" alt="">
                    <h2>¡Gracias por confiar en nosotros!</h2> 
                    <p>En momentos te llamaremos, si ves que es un número desconocido,<b>somos nosotros</b>, contesta con confianza. </p>
                </div>`;
                                                $('.loaderCR').css("opacity", "1");
                                                // Form submit
                                                $.ajax({
                                                    url: 'https://wom-co.convertia.com/public/integration/process',
                                                    type: 'POST',
                                                    contentType: 'application/json',
                                                    dataType: 'json',
                                                    data: JSON.stringify(data),
                                                    crossDomain: true,
                                                    processData: false,
                                                    success: function (response) {
                                                        if (response.status) {
                                                            $('.loaderCR').css("opacity", "0");
                                                            respuesta.innerHTML = okRespuesta;
                                                        } else {
                                                            console.log(response.description);
                                                        }
                                                        console.log(response.status);
                                                    }
                                                });
                                            }
                                        });


                                        jQuery.validator.addMethod("phoneStarting3", function (phone_number, element) {
                                            return this.optional(element) || phone_number.match(/^3\d{9}$/);
                                        });
                                        // Validacion nombre
                                        jQuery.validator.addMethod("lettersonly", function (value, element) {
                                            return this.optional(element) ||
                                                /^[a-zA-ZÀ-ÿ\u00f1\u00d1]+(\s*[a-zA-ZÀ-ÿ\u00f1\u00d1]*)*[a-zA-ZÀ-ÿ\u00f1\u00d1]+$/i
                                                    .test(value);
                                        });
                                        // validar email
                                        $.validator.addMethod('betterEmail', function (value, element) {
                                            return this.optional(element) ||
                                                /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
                                                    .test(value);
                                        }, "Ingresa un correo valido.");

                                        // validar selects
                                        $.validator.addMethod("valueNotEquals", function (value, element, arg) {
                                            return arg !== value;
                                        }, "elige una opcion.");

                                    });

                                </script>
                            </div>
                        </div>
