                            <script>
                                requirejs([
                                    'jquery',
                                ], function ($) {

                                    $(document).ready(function () {
                                        // accordeones FAQ
                                        $(".CMSPreguntasDetails1").click(function () {
                                            $(this).toggleClass("activo");
                                            $(this).siblings(".CMSPreguntasTextDetails1").toggle(200);
                                            $(this).parents(".cardFAQ").siblings().find(".CMSPreguntasDetails1").removeClass("activo");
                                            $(this).parents(".cardFAQ").siblings().find(".CMSPreguntasTextDetails1").hide(200);
                                        });

                                        // accordeones TyC
                                        $(".navSub p").click(function () {
                                            $(this).parent(".navSub").toggleClass("activoNavSub");
                                            $(this).parent(".navSub").find(".navSubBlock").toggle(200);

                                        });
                                        // SECCION APN
                                        $(".verAPN").click(function () {
                                            $(".bloque-links-apn").addClass("bloque-links-apn-activa");
                                        })
                                        $(".bloque-links-apn li:first-child").addClass("APNactiva");
                                        $(function () {
                                            $('.bloque-links-apn li').on('click', function () {
                                                show_content($(this).index());
                                                $(".bloque-links-apn").removeClass("bloque-links-apn-activa");
                                            });
                                            show_content(0);
                                            function show_content(index) {
                                                // Make the content visibles

                                                $('.cajaAPN').removeClass('cajaActiva');
                                                $('.cajaAPN:nth-of-type(' + (index + 1) + ')').addClass('cajaActiva');
                                                // Set the tab to selected
                                                $(".bloque-links-apn li").click(function () {
                                                    $(this).addClass("APNactiva");
                                                    $(this).siblings().removeClass("APNactiva");
                                                });
                                            }
                                        });
                                    });
                                });
                            </script>
