                              }); */
                    //$("#phnumber").bind('keyup', function() {
                    $(document).ready(function ($) {
                        var str = $("#phnumber").val();
                        //alert(str);
                        var ua = navigator.userAgent;
                        var isAndroid = /Android/i.test(ua);
                        //var isChrome = /Chrome/i.test(ua);
                        //  if (isAndroid && str.match(/^\d+$/) && str != '') {
                        if (isAndroid) {
                            $("#phnumber").attr("type", "tel");
                            $("#phnumber").attr("pattern", "[0-9]*");
                            // $("#actmsisdn").attr("maxlength","");
                        } else if (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream) {
                            $("#phnumber").attr("type", "text");
                            $("#phnumber").attr("pattern", "[0-9]*");
                        }

                        var phones = [{ "mask": "(###)###-####" }, { "mask": "(###) ###-##############" }];
                        if (str != null || str != '') {
                            //alert(str);
                            $('#phnumber').inputmask({
                                mask: phones,
                                greedy: false,
                                definitions: { '#': { validator: "[0-9]", cardinality: 1 } }
                            });
                        }
                    });


                    $("a[href$='/infoimportant']").addClass("ddarrow");
                    $(document).on('mouseenter', '.ui-menu-item', function (event) {

                        var hyperLink = $(this).find('a').attr("href");
                        if (!hyperLink) {
                            hyperLink = '#';
                        }
                        var linkArr = hyperLink.split("/");
                        if (linkArr.includes("infoimportant") || $(this).hasClass("parent") || $(this).find('a').hasClass("infoimportantmenu")) {
                            $('.megamenuMainBlock').show();
                            $(this).find('a:first').attr("href", "#");
                            $(this).find('a:first').addClass("infoimportantmenu");
                            $(this).find('a:first').addClass("ddarrow");
                        }
                        else {
                            $('.megamenuMainBlock').hide();
                        }

                    }).on('mouseleave', '.megamenuMainBlock,.ui-menu-item', function () {
                        if ($(this).hasClass("megamenuMainBlock")) {
                            $('.megamenuMainBlock').hide();
                        }
                        if ($(this).hasClass("parent") || $(this).hasClass("megamenuMainBlock")) {
                            $(this).find('a:first').removeClass("ddarrow");
                        }

                    });
                    $(document).on('mouseenter', '.megamenuMainBlock', function (event) {

                        $('.infoimportantmenu').addClass("ddarrow");

                    }).on('mouseleave', '.megamenuMainBlock', function () {
                        $('.infoimportantmenu').removeClass("ddarrow");
                    });
                    /* $(document).on('click','.ui-menu-item a', function (event) {
                    var hyperLink = $(this).attr("href");
                    if(!hyperLink){
                        hyperLink = '#';
                    }
                    var linkArr = hyperLink.split("/");
                        if(linkArr.includes("infoimportant") || $(this).hasClass("infoimportantmenu")){
                            $('.megamenuMainBlock').toggle();
                            $(this).attr("href","#");
                            $(this).addClass("infoimportantmenu");
                        }
                    }); */

                });
            </script>
