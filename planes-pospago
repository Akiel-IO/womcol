                        <script type="text/javascript">
                            var isGtmEnabled = '1';
                            var homeImpressionJson = [];
                            var homeImpressionKeyVal = [];
                            var maxPerPage;
                            require([
                                'jquery',
                                'Magento_Ui/js/modal/modal',
                                'mage/url',
                                'Magento_Customer/js/customer-data',
                                'domReady!'
                            ], function ($, modal, url, customerData) {
                                'use strict';
                                var options = {
                                    type: 'popup',
                                    modalClass: 'logoutModal',
                                    responsive: true,
                                    innerScroll: true,
                                    buttons: [{
                                        text: $.mage.__('Cancelar'),
                                        class: 'close-modal btn-logoutcancel',
                                        click: function () {
                                            this.closeModal();
                                        }
                                    },
                                    {
                                        text: $.mage.__('Cerrar sesión'),
                                        class: 'closeModal btn-accountlogout',
                                        click: function () {
                                            var logOutUrl = url.build('customer/account/logout');
                                            window.location.href = logOutUrl;
                                        }
                                    }]
                                };
                                $(document).ready(function () {
                                    var customer = customerData.get('customer');
                                    customer.subscribe(function (updatedCustomer) {
                                        var updatedCustomerName = updatedCustomer.fullname;
                                        if (updatedCustomerName) {
                                            $('.mLogBlock').addClass('mAfterLogin1  deskAfterLogin1');
                                        } else {
                                            $('.mLogBlock').removeClass('mAfterLogin1  deskAfterLogin1');
                                        }
                                    }, this);
                                    var popup = modal(options, $('#logout-popup-modal'));
                                    $(".link.authorization-link.logout").click(function () {
                                        $("#logout-popup-modal").modal(options).modal("openModal");
                                        return false;
                                    });


                                    // for google tag manager begins

                                    $("a.level-top,.megamenu-item,.megamenu-subitem,.topLeftLinks li a,.authorization-link.login a,ul.mLogBlock:first li:last a,ul.mLogBlock:eq(1) li:last a").click(function (event) {
                                        //event.preventDefault();
                                        var gtmDynamicEventValue = $(this).text();
                                        dataLayer.push({
                                            'event': 'menu header',
                                            'Value': gtmDynamicEventValue,
                                            'ecommerce': undefined
                                        });
                                    });
                                    $(".footerLinksMain a").click(function (event) {
                                        //event.preventDefault();
                                        var gtmDynamicEventValue = $(this).text();
                                        dataLayer.push({
                                            'event': 'footer',
                                            'Value': gtmDynamicEventValue
                                        });
                                    });
                                    // gtm code for product click on homepage
                                    // gtm code for slider pagination on home page

                                    $('body').on('click', '.slick-dots li button', function () {
                                        //alert($(this).);
                                        if ($(this).parent().parent().parent().hasClass("pagebuilder-slider")) {
                                            return;
                                        }
                                        var pageIndex = $(this).parent().index();
                                        var pageno = parseInt(pageIndex) + 1;
                                        var totalrecords;
                                        var perpageallowed = maxPerPage;
                                        var limit;
                                        //if($(this).parent().parent().parent().parent().parent().parent().hasClass("postpaidPlansBlockMain")){
                                        // new class to be created on page builder 
                                        if ($(this).parent().parent().parent().parent().hasClass("gtmDataPospago")) {

                                            var pospagoJsonParsed = JSON.parse(homeImpressionJson[0]);
                                            totalrecords = pospagoJsonParsed.length;
                                            var remaining = totalrecords - (pageno - 1) * perpageallowed;
                                            //var totalpages = totalrecords/maxPerPage;
                                            var startfrom = (pageno - 1) * perpageallowed;
                                            //alert(startfrom);
                                            if (remaining > perpageallowed) {
                                                limit = perpageallowed;
                                            }
                                            else {
                                                limit = remaining;
                                            }
                                            var clickPospagoDatalayerValue = pospagoJsonParsed.slice(startfrom, startfrom + limit);
                                            //console.log(clickPospagoDatalayerValue);
                                            dataLayer.push({
                                                'ecommerce': {
                                                    'impressions': undefined
                                                }
                                            });
                                            dataLayer.push({
                                                'event': 'productImpression',
                                                'ecommerce': {
                                                    'currencyCode': 'COP',
                                                    'impressions': clickPospagoDatalayerValue
                                                }
                                            });

                                        }
                                        //if($(this).parent().parent().parent().parent().parent().parent().hasClass("prepaidPlansBlockMain")){
                                        // new class to be created in page builder
                                        if ($(this).parent().parent().parent().parent().hasClass("gtmDataPrepago")) {
                                            var prepagoJsonParsed = JSON.parse(homeImpressionJson[1]);
                                            totalrecords = prepagoJsonParsed.length;
                                            var remaining = totalrecords - (pageno - 1) * perpageallowed;
                                            //var totalpages = totalrecords/maxPerPage;
                                            var startfrom = (pageno - 1) * perpageallowed;
                                            //alert(startfrom);
                                            if (remaining > perpageallowed) {
                                                limit = perpageallowed;
                                            }
                                            else {
                                                limit = remaining;
                                            }
                                            var clickPrepagoDatalayerValue = prepagoJsonParsed.slice(startfrom, startfrom + limit);
                                            //console.log(clickPrepagoDatalayerValue);
                                            dataLayer.push({
                                                'ecommerce': {
                                                    'impressions': undefined
                                                }
                                            });
                                            dataLayer.push({
                                                'event': 'productImpression',
                                                'ecommerce': {
                                                    'currencyCode': 'COP',
                                                    'impressions': clickPrepagoDatalayerValue
                                                }
                                            });
                                        }
                                        //if($(this).parent().parent().parent().parent().parent().parent().hasClass("internetPlansBlockMain")){
                                        // new class to be created in the page builder
                                        if ($(this).parent().parent().parent().parent().hasClass("gtmDataInternetHogar")) {

                                            var internethogarJsonParsed = JSON.parse(homeImpressionJson[2]);
                                            totalrecords = internethogarJsonParsed.length;
                                            var remaining = totalrecords - (pageno - 1) * perpageallowed;
                                            //var totalpages = totalrecords/maxPerPage;
                                            var startfrom = (pageno - 1) * perpageallowed;
                                            //alert(startfrom);
                                            if (remaining > perpageallowed) {
                                                limit = perpageallowed;
                                            }
                                            else {
                                                limit = remaining;
                                            }
                                            var clickIntHogarDatalayerValue = internethogarJsonParsed.slice(startfrom, startfrom + limit);
                                            //console.log(clickIntHogarDatalayerValue);
                                            dataLayer.push({
                                                'ecommerce': {
                                                    'impressions': undefined
                                                }
                                            });
                                            dataLayer.push({
                                                'event': 'productImpression',
                                                'ecommerce': {
                                                    'currencyCode': 'COP',
                                                    'impressions': clickIntHogarDatalayerValue
                                                }
                                            });
                                        }


                                        //alert(limit);
                                    });
                                    // for google tag manager ends

                                });
                            });
                        </script>
