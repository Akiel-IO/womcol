            <script>
                require(['jquery', 'Magento_Ui/js/modal/modal'], function ($, modal) {
                    $(document).ready(function () {
                        $('.static-block-message').hide();

                        $('body').on('click', '.open-form', function () {
                            $('#custompopup').toggle();
                            $('.clickToCallOpen').hide();
                            $('.close-1').toggleClass("clickToCallClose");
                            $('.close-2').toggleClass("clickToCallClose");
                        });
                        $("#pemail").focusout(function () {
                            var str = $("#pemail").val();
                            var regex = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
                            if (regex.test(str) == false) {
                                $('#pemail-error').html('');
                                $('#pemail').after('<div id="pemail-error" class="mage-error" generated="true">Por favor ingresa un e-mail válido</div>');
                                return false;
                            } else {
                                $('#pemail-error').html('');
                                $("#pemail-error").hide();
                            }
                        });

                        $("#celular").focusout(function () {
                            var str = $("#celular").val();
                            if (str != '') {
                                $("#celular").addClass("validate-digits");
                                $("#celular").addClass("tendigitline");
                                $("#celular").addClass("lineStartingWith3");
                                if (str.length != '10') {
                                    $('#celular-error').html('');
                                    $('#celular').after('<div id="celular-error" class="mage-error" generated="true">Por favor Ingresa un número válido </div>');
                                    return false;
                                } else {
                                    $('#celular-error').html('');
                                    $("#celular-error").hide();
                                }
                                var numberRegex = /^3\d{8,}$/;
                                if (numberRegex.test(str) == false) {
                                    $('#celular-error').html('');
                                    $('#celular').after('<div id="celular-error" class="mage-error" generated="true">Por favor Ingresa un número válido </div>');
                                    return false;
                                } else {
                                    $('#celular-error').html('');
                                    $("#celular-error").hide();
                                }
                            }
                        });

                        $("#checkboxpopup").click(function () {
                            checked = $('#checkboxpopup').val();
                            if (checked == 0) {
                                $('#checkboxpopup').val('1');
                            } else {
                                $('#checkboxpopup').val('0');
                            }
                        });
                        $("#popup-form").submit(function () {

                            if (($('#checkboxpopup').val()) == 0) {
                                $('#popup-error-msg1').css('display', 'block');
                                $('#popup-error-msg1').css('color', '#df280a');
                                $('#popup-error-msg1').css('font-size', '1.2rem');
                                return false;
                            } else {
                                $('#popup-error-msg1').css('display', 'none');
                                //return true;
                            }

                            if (($('#g-recaptcha-response').val()) === '') {
                                $('#popup-error-msg').css('display', 'block');
                                $('#popup-error-msg').css('color', '#df280a');
                                $('#popup-error-msg').css('font-size', '1.2rem');
                                return false;
                            }
                            else {
                                $('#popup-error-msg').css('display', 'none');
                            }

                            if ($('#popup-form').valid()) {
                                var fname = $("#pnombres").val();
                                //var lname  = $("#papellidos").val();
                                var mail = $("#pemail").val();
                                var tel = $("#celular").val();
                                var cityy = $("#pciudad").val();
                                var interesado = $("#selform").val();
                                var actualmente = $("#actualmenteTengo").val();
                                var operador = $("#operadorActual").val();
                                var accept = $("#checkboxpopup").val();
                                var formdevice = $("#formDevice").val();
                                var furl = "https://www.wom.co/planes-pospago";
                                var url = "https://www.wom.co/form/index/popup";
                                // datalayer code for GTM starts
                                var operadorText = $("#operadorActual option:selected").text();
                                dataLayer.push({
                                    'event': 'formularioheader',
                                    'estoyinteresado': interesado,
                                    'actualmentetengo': actualmente,
                                    'operadoractual': operadorText
                                });
                                //console.log(dataLayer);
                                // datalayer code GTM ends

                                $.ajax({
                                    url: url,
                                    type: "POST",
                                    data: { firstname: fname, email: mail, phone: tel, city: cityy, estoyinteresado: interesado, actualmentetengo: actualmente, operadoractual: operador, acepta: accept, formname: formdevice, formurl: furl },
                                    showLoader: true,
                                    cache: false,
                                    success: function (response) {
                                        var status = response.output;
                                        console.log(response.output);
                                        $('#checkboxpopup').val('0');
                                        if (status == '1') {
                                            $('#custompopup').hide();
                                            $('#popup-form')[0].reset();
                                            var options = {
                                                type: 'popup',
                                                responsive: true,
                                                innerScroll: true,
                                                modalClass: 'customPopupMain',
                                                buttons: [{
                                                    text: $.mage.__('FINALIZAR'),
                                                    class: 'customPopupx popup-llamesuccess',
                                                    id: 'close',
                                                    click: function () {
                                                        this.closeModal();
                                                    }
                                                }]
                                            };

                                            var popup = modal(options, $('#popup-modal-head-success'));
                                            $('#popup-modal-head-success').modal('openModal');
                                        } else {
                                            $('#custompopup').hide();
                                            $('#popup-form')[0].reset();
                                            $('.error-message').text(response.output);
                                            var options = {
                                                type: 'popup',
                                                responsive: true,
                                                innerScroll: true,
                                                modalClass: 'customPopupMain',
                                                buttons: [{
                                                    text: $.mage.__('FINALIZAR'),
                                                    class: 'customPopupx popup-llameclose',
                                                    id: 'close',
                                                    click: function () {
                                                        this.closeModal();
                                                    }
                                                }]
                                            };
                                            var popup = modal(options, $('#popup-modal-head-fail'));
                                            $('#popup-modal-head-fail').modal('openModal');
                                        }
                                    }
                                });
                            }
                            return false;
                        });
                        /*   $("#pnombres, #pemail, #celular, #pciudad, #selform, #actualmenteTengo, #operadorActual").blur(function(){
                          $('#popup-form').valid();
                          
                          }); */

                        $("operadorActual").change(function () {
                            if ($(this).val() == 'Seleccione' || $(this).val() == '') {
                                $("#operadorActual").val($("#operadorActual option:first").val());
                            }
                        });

                    });
                });
            </script>
            <script type="text/javascript">
                require(['jquery'], function ($) {
                    $('#pciudad').on('keyup', function (event) {
                        var container, input, filter, li, input_val;
                        container = $('#pciudad').closest(".searchableclick");
                        input_val = container.find("#pciudad").val().toUpperCase();

                        if (["ArrowDown", "ArrowUp", "Enter"].indexOf(event.key) != -1) {
                            keyControls(event, container)
                        } else {
                            li = container.find("ol li");
                            li.each(function (i, obj) {
                                if ($(this).text().toUpperCase().indexOf(input_val) > -1) {
                                    $(this).show();
                                } else {
                                    $(this).hide();
                                }
                            });

                            container.find("ol li").removeClass("selected");
                            setTimeout(function () {
                                container.find("ol li:visible").first().addClass("selected");
                            }, 100)
                        }
                    });

                    function keyControls(e, container) {
                        if (e.key == "ArrowDown") {

                            if (container.find("ol li").hasClass("selected")) {
                                if (container.find("ol li:visible").index(container.find("ol li.selected")) + 1 < container.find("ol li:visible").length) {
                                    container.find("ol li.selected").removeClass("selected").nextAll().not('[style*="display: none"]').first().addClass("selected");
                                }

                            } else {
                                container.find("ol li:first-child").addClass("selected");
                            }

                        } else if (e.key == "ArrowUp") {

                            if (container.find("ol li:visible").index(container.find("ol li.selected")) > 0) {
                                container.find("ol li.selected").removeClass("selected").prevAll().not('[style*="display: none"]').first().addClass("selected");
                            }
                        } else if (e.key == "Enter") {
                            container.find("#pciudad").val(container.find("ol li.selected").text()).blur();
                            //alert(container.find("ul li.selected").text())
                        }

                        container.find("ol li.selected")[0].scrollIntoView({
                            behavior: "smooth",
                        });
                    }

                    $(".searchableclick input").focus(function () {
                        $(this).closest(".searchableclick").find("ol").show();
                        $(this).closest(".searchableclick").find("ol li").show();
                    });
                    $(".searchableclick input").blur(function () {
                        let that = this;
                        setTimeout(function () {
                            $(that).closest(".searchableclick").find("ol").hide();
                        }, 300);
                    });

                    $(document).on('click', '.searchableclick ol li', function () {
                        $(this).closest(".searchableclick").find("#pciudad").val($(this).text()).blur();

                    });

                    $(".searchableclick ol li").hover(function () {
                        $(this).closest(".searchableclick").find("ol li.selected").removeClass("selected");
                        $(this).addClass("selected");
                    });
                });
            </script>

