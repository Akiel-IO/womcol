                        require(['jquery', 'Magento_Ui/js/modal/modal'], function ($, modal) {
                            $(".staicContentTab").hide();
                            var options = {
                                type: 'popup',
                                responsive: true,
                                innerScroll: true,
                                modalClass: 'customPopupMain',
                                buttons: [{
                                    text: $.mage.__('Cerrar'),
                                    class: 'customPopupx',
                                    id: 'close',
                                    click: function () {
                                        this.closeModal();
                                    }
                                }]
                            };
                            $(document).ready(function () {

                                var popup = modal(options, $('#popup-modal'));
                                $('.tocart').unbind('click').bind('click', function (e) {
                                    //var id = $(this).attr('id');
                                    //var number = id.split(/([0-9]+)/);
                                    $('#popup-modal').modal(options).modal('openModal');
                                    return false;
                                });
                            });

                            $('#nuevo').click(function () {
                                $("#productsTab").show();
                                $("#staicContentTab").hide();
                                $("#prepaidDescription").show();
                                $("#nuevo").addClass('active');
                                $("#wom").removeClass('active');
                            });

                            $('#wom').click(function () {
                                $("#staicContentTab").show();
                                $("#productsTab").hide();
                                $("#prepaidDescription").hide();
                                $("#wom").addClass('active');
                                $("#nuevo").removeClass('active');
                            });

                        });
                    </script>
